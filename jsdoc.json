{
    "plugins": [],
    "source": {
        "include": [
            "src"
        ],
        "includePattern": "(.ts$|.tsx$)",
        "excludePattern": "(node_modules|stories|docs)"
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "template": "templates/default",
        "destination": "./docs/",
        "recurse": "true"
    }
}